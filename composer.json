{
  "name": "adrmrn/simple-collections",
  "type": "library",
  "description": "Library with simple generics collections inspired by Java/C#.",
  "keywords": ["generics", "collection"],
  "license": "MIT",
  "require": {
    "php": "^8.2"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.1",
    "phpro/grumphp": "^1.16",
    "squizlabs/php_codesniffer": "^3.7",
    "phpstan/phpstan": "^1.10"
  },
  "authors": [
    {
      "name": "Adrian M",
      "homepage": "https://github.com/adrmrn",
      "role": "Developer"
    }
  ],
  "autoload": {
    "classmap": [],
    "psr-4": {
      "Adrmrn\\SimpleCollections\\": "src/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ],
    "psr-4": {
      "Adrmrn\\SimpleCollections\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "post-install-cmd": [
      "composer dump-autoload"
    ]
  }
}
